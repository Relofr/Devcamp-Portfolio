<div class="inner cover">
  <h1><div class="container-fluid technewsfont">
    Tech News</h1>
  </div>

  <div class="row">
    <% @tweets.each do |tweet| %>
      <div class="col-md-6">
        <div class="tech-news">
          <div class="card">
            <%= twitter_parser tweet %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>